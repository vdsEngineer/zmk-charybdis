        next_window: next_window {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(LG(DOWN))>;
            label = "NEXT_WINDOW";
        };

        pre_window: pre_window {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(LG(UP))>;
            label = "PRE_WINDOW";
        };

        open_left_window: open_left_window {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(LEFT)>;
